{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-plusplus": "off",
    "no-prototype-builtins": "off"
  }
}
